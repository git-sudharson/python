list1 = []
n = int(input("Enter the list size "))

print("\n")
for i in range(0, n):
    print("Enter number at index", i, )
    item = int(input())
    list1.append(item)
print("User list is ", list1)
print("enter key")
key=int(input())
def linear_Search(list1,n,key):
    for i in range(0,n):
        if(list1[i]==key):
            return i
    return -1
res=linear_Search(list1,n,key)
if(res == -1):
    print("Element not found")
else:
    print("Element fount at index:",res)

